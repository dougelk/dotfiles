"nmap <silent> <Right>      :cnext<CR>
"nmap <silent> <Up>         :cnf<CR>
"nmap <silent> <Left>       :cprev<CR>
"nmap <silent> <Down>       :cpf<CR>

"set listchars=tab:▸\ ,eol:¬
"set list

filetype plugin indent on
set wildmenu
set number

" search settings
set hlsearch
set ignorecase
set smartcase
set incsearch
